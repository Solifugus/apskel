	// *** Process {{variablized_request}} Requests 
	public function process{{classifized_request}}( $param, $missing ) {
		//if( !isset( $param['messages'] ) ) { $param['messages'] = ''; } 
		if( !isset( $param['warnings'] ) ) { $param['warnings'] = ''; } 
		$param['warnings'] .= $missing;

		if( $param['fresh'] !== true ) {
			// Validate inputs
			$is_bad = false;
			$is_bad_reason = '';

			// TODO: Write validation rules.  Make $is_bad == false if any fail and put the human readable reason in $is_bad_reason

	
			// If invalid then provide warnings as to why..
			if( $is_bad === true ) {
				$param['warnings'] .= $is_bad_reason;
			}
			else {
				// TODO: Do whatever happens when validation is successful
			}
		} // end of validating input (if other than fresh call to page)
		return $this->views->compose{{classifized_request}}( $param );
	}
	
